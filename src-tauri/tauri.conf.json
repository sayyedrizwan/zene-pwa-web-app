{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "beforeBuildCommand": "npm run build",
    "beforeDevCommand": "npm run dev",
    "devPath": "http://localhost:3419",
    "distDir": "../build"
  },
  "package": {
    "productName": "Zene",
    "version": "0.1.0"
  },
  "tauri": {
    "macOSPrivateApi": true,
    "allowlist": {
      "all": false
    },
    "bundle": {
      "active": true,
      "category": "Music",
      "copyright": "Zene | Rizwan Sayyed",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "../static/logo76.png",
        "../static/logo120.png",
        "../static/logo152.png",
        "../static/logo167.png",
        "../static/logo180.png",
        "../static/logo820.png",
        "../static/icon.svg",
        "../static/favicon.svg"
      ],
      "identifier": "com.rizwansayyed.zene",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "Introducing Zene a free music app, the ultimate music experience that redefines the way you listen to your favorite tunes. Dive into a world of limitless music with our free app, offering ad-free access to over 1 billion songs, albums, playlists, podcasts, covers, remixes, and more.",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "transparent": true,
        "fullscreen": true,
        "height": 600,
        "resizable": true,
        "title": "Zene: A Free Music App",
        "width": 800
      }
    ]
  }
}
